<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SubmitInput</servlet-name>
        <servlet-class>SubmitInput</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewQuery</servlet-name>
        <servlet-class>NewQuery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetResults</servlet-name>
        <servlet-class>GetResults</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitInput</servlet-name>
        <url-pattern>/SubmitInput</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewQuery</servlet-name>
        <url-pattern>/NewQuery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetResults</servlet-name>
        <url-pattern>/GetResults</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
